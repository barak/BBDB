@SET_MAKE@

MAKEINFO = @MAKEINFO@

all: info bbdb.dvi

info: bbdb.info

bbdb.info: bbdb.texinfo
	@if test "x$(MAKEINFO)" = "x" ; then                 \
           echo "Sorry, no \`makeinfo' program available." ; \
         else                                                \
           $(MAKEINFO) bbdb.texinfo ;                        \
         fi

bbdb.dvi: bbdb.texinfo
	@if test "x$(TEXI2DVI)" = "x" ; then                 \
           echo "Sorry, no \`texi2dvi' program available." ; \
         else                                                \
           $(TEXI2DVI) bbdb.texinfo ;                        \
         fi

bbdb.pdf: bbdb.texinfo
	@if test "x$(TEXI2DVI)" = "x" ; then                 \
           echo "Sorry, no \`texi2dvi' program available." ; \
         else                                                \
           $(TEXI2DVI) -p bbdb.texinfo ;                     \
         fi

clean:
	$(RM) -f *.aux *.cp *.cps *.dvi *.pdf *.fn *.fns *.ky *.log *.pg \
	  *.toc *.tp *.vr *.vrs

reallyclean: distclean

distclean: clean
	$(RM) -f bbdb.info*

cvsclean: distclean
	$(RM) -f Makefile

